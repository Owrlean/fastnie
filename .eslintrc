{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "no-multiple-empty-lines": "error"
    },
    "parser": "@babel/eslint-parser",
    "globals": {
        "ReactDOM": false,
        "React": false,
        "PropTypes": false
    }
}